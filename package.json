{
  "name": "node-express-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "rebuild": "rm -rf dist && npm run build",
    "start": "node --env-file=.env dist/main.js",
    "start:dev": "npm run rebuild && npm run start"
  },
  "dependencies": {
    "express": "^4.21.2",
    "ioredis": "^5.5.0",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/pg": "^8.11.11",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3"
  },
  "prettier": {
    "printWidth": 140,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
